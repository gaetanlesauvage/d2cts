@inproceedings{Lesauvage2011,
 title = {{D$^{2}$CTS} : A Dynamic and Distributed Container Terminal Simulator},
 author = {Lesauvage, Ga{\"e}tan and Balev, Stefan and Guinand, Fr{\'e}d{\'e}ric},
 booktitle = {HMS 2011 : The $13^{rd}$ International Conference on Harbor, Maritime {\&} Multimodal Logistics Modelling and Simulation},
