@article {Laporte1988,
    author       = {Laporte, Gilbert and Nobert, Yves and Taillefer, Serge},
    title        = {Solving a family of multi-depot vehicle routing and location-routing problems.},
    year         = {1988},
    journal      = {Transportation Science},
    volume       = {22},
    number       = {3},
    issn         = {0041-1655},
    pages        = {161-172},
    publisher    = {INFORMS, Hanover, MD},
    doi          = {10.1287/trsc.22.3.161},
    abstract     = {The authors consider three asymmetric multi-depot vehicle routing and location-routing problems, under capacity and cost constraints. They show how, after an appropriate graph transformation, these problems can be formulated in a unified way as constrained assignment problems. A solution approach based on the branch-and-bound method by {\it G. Carpaneto} and {\it P. Toth} [Manage. Sci. 26, 736-743 (1980; Zbl 0445.90089)] for the asymmetric TSP is presented. The subproblems that arise are all assignment problems and can thus be solved efficiently. The various side constraints are taken into acount in the branching process, by fixing some of the variables at zero or one, thereby preserving the assignment structure. Computational results indicate that problems of relatively large sizes can be solved to optimality.},
    reviewer     = {M.Savelsbergh},
    identifier   = {04083349},
}
