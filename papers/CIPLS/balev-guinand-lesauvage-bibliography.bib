@ARTICLE {Deneubourg1983,
  title = {Probabilistic behaviour in ants: A strategy of errors?},
  author = {Deneubourg, Jean-Louis and Pasteels, Jacques M.  and Verhaeghe, J. C. },
  journal = {Journal of Theoretical Biology},
  year = {1983},
  volume = {105},
  pages = {259--271},
  doi = {10.1016/s0022-5193(83)80007-1},
  abstract = {Animal behaviour is probabilistic. This is exemplified by the communication behaviour of ants during food-searching. Experimental evidence demonstrates that species differ in the accuracy of their recruitment. We show here, with the help of a very simple mathematical model, that the randomness of of behaviour can have an adaptative advantage for ants. The model demonstrates that the degree of randomness could be optimally "tuned" to particular ecological conditions, such as food quantity and distribution.},
annote={Long lasting structures lack the plasticity needed in a fluctuating environment and high rates of development of rigid structures cost energy. In this situation,
 one possible reaction of the society is to try to eliminate externam fluctuations. One way is to damp the external fluctuations with homeostatic mechanisms: the colony tries to minimize the external fluctuations by appropriate behaviours (stock management,
 temperature control,
...). Another possible reaction to fluctuating environment is the development of temporary structure (e.g. recruitment trail) geberated by a self-organization process. With the help of a mathematical model,
 we suggest that a certain amount of noise during recruitment could play an adaptative role for the society. Clearly more noise is advantageous when the food is parcelled,
 and when each source is less rewarding.},
date-modified={2005-12-08 10:10:56 +0100},
groups={Self-organization},
owner={Simon Garnier},
printed={Yes},
read={Yes},
}
