@article{Jain1999,
    abstract = {{A great deal of research has been focused on solving the job-shop problem (P J), over the last forty years, resulting in a wide variety of approaches. Recently, much effort has been concentrated on hybrid methods to solve P J as a single technique cannot solve this stubborn problem. As a result much effort has recently been concentrated on techniques that combine myopic problem specific methods and a meta-strategy which guides the search out of local optima. These approaches currently provide the best results. Such hybrid techniques are known as iterated local search algorithms or meta-heuristics. In this paper we seek to assess the work done in the job-shop domain by providing a review of many of the techniques used. The impact of the major contributions is indicated by applying these techniques to a set of standard benchmark problems. It is established that methods such as Tabu Search, Genetic Algorithms, Simulated Annealing should be considered complementary rather than competitive. In addition this work suggests guide-lines on features that should be incorporated to create a good P J system. Finally the possible direction for future work is highlighted so that current barriers within P J maybe surmounted as we approach the 21st Century.}},
    author = {Jain, Anant S. and Meeran, Sheik},
    citeulike-article-id = {3757159},
    citeulike-linkout-0 = {http://citeseerx.ist.psu.edu/viewdoc/summary?doi=10.1.1.54.8522},
    journal = {European Journal of Operations Research},
    keywords = {job, phd, review, scheduling},
    number = {113},
    pages = {390--434},
    posted-at = {2008-12-08 15:45:45},
    priority = {2},
    title = {A STATE-OF-THE-ART REVIEW OF JOB-SHOP SCHEDULING TECHNIQUES},
    url = {http://citeseerx.ist.psu.edu/viewdoc/summary?doi=10.1.1.54.8522},
    year = {1999}
}
